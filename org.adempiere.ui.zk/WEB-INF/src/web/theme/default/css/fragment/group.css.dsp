tr.z-group {
	background: #a1c2fa29;
    background-image: none !important;
}

.z-group-header .z-label {
	color: #3367d6;
    width: auto;
    font-size: 12px !important;
    font-weight: 400;
}

.z-group td.z-group-inner {
	overflow: hidden !important;
	border-bottom: 1px solid #a1c2fa29 !important;
	border-top: 1px solid #a1c2fa29 !important;
	padding: 1px 2px;
	background: none;
	background-color: #a1c2fa29;
    color: #3367d6;
}

.z-group-icon {
	display: inline-block;
	font-family: FontAwesome;
	font-weight: normal;
	font-style: normal;
	color: #3367d6;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.z-group-icon-close:before {
	content: "\f0da";
}
.z-group-icon-open:before {
	content: "\f0d7";
}
